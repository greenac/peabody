{
  "compilerOptions": {
    "target": "es2016",
    "lib": [
      "es6",
      "es2015",
      "es2016",
      "es2017",
      "es2018",
      "esnext",
      "esnext.asynciterable"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react"
  },
  "include": [
    "src",
    "index.ts"
  ]
}
